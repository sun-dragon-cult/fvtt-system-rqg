<h2>ğŸ›Active EffectsğŸ§ª</h2>
<table>
  <thead>
  <tr>
    <th class="text-left">Label</th>
    <th class="text-left">Disabled</th>
    <th class="text-left">Start Time</th>
    <th class="text-left">Key - Value - Mode</th>
    <th class="text-left"></th>
  </tr>
  </thead>
  {{#each data.effects}}
  <tbody data-effect-id="{{_id}}">
  <tr data-actor-effect-edit>
    <td>{{label}}</td>
    <td>{{yes-no disabled}}</td>
    <td>{{duration.startTime}}</td>
    <td>
      {{#each changes}}
        {{key}} - {{value}} - {{mode}}<br>
      {{/each}}</td>
    <td class="text-right"><a title="Delete Effect" data-actor-effect-delete><i class="fas fa-trash"></i></a></td>
    </tr>

  </tbody>
  {{/each}}

</table>
