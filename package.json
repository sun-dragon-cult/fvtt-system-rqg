{
  "private": true,
  "name": "rqg",
  "version": "3.4.8",
  "type": "module",
  "esModuleInterop": true,
  "description": "",
  "scripts": {
    "build": "run-s clean:dist build:system build:packs",
    "build:packs": "node --experimental-specifier-resolution=node --loader ts-node/esm -r tsconfig-paths/register buildScripts/buildPacks.ts",
    "build:system": "run-s bundle build:template analyze",
    "bundle": "pnpm snowpack build",
    "build:template": "node jsTemplate2json.mjs",
    "compile:noemit": "tsc --noEmit",
    "lint": "eslint ./src --fix",
    "analyze": "run-p compile:noemit lint",
    "test": "jest",
    "test:coverage": "jest --coverage --collectCoverageFrom=./src/**",
    "clean:dist": "rimraf --glob dist src/assets/packs/*",
    "clean:deps": "rimraf node_modules",
    "clean": "run-p clean:dist clean:deps",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm"
  },
  "author": "",
  "license": "UNLICENCED",
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@league-of-foundry-developers/foundry-vtt-types": "0.8.9-2",
    "@snowpack/plugin-sass": "^1.4.0",
    "@snowpack/plugin-typescript": "^1.2.1",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "4.0.9",
    "@types/node": "^20.14.14",
    "@types/ws": "^8.18.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "chalk": "^5.4.1",
    "classic-level": "^1.3.0",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "fs-extra": "^11.3.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.4.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "sass": "^1.85.1",
    "socket.io": "^4.8.1",
    "snowpack": "^3.8.8",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "foswig": "^3.0.1"
  }
