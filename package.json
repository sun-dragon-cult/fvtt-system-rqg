  "scripts": {
    "old-build": "run-s clean:dist build:system build:packs",
    "build:packs": "node --experimental-specifier-resolution=node --loader ts-node/esm -r tsconfig-paths/register buildScripts/buildPacks.ts",
    "build:system": "run-s bundle build:template analyze",
    "bundle": "pnpm snowpack build",
    "build:template": "node jsTemplate2json.mjs",
    "compile:noemit": "tsc --noEmit",
    "old-lint": "eslint ./src --fix",
    "analyze": "run-p compile:noemit lint",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "clean:dist": "rimraf --glob dist src/assets/packs/*",
    "clean:deps": "rimraf node_modules",
    "clean": "run-p clean:dist clean:deps",
    "prepare": "husky",
    "preinstall": "npx only-allow pnpm",
    "build:dev": "vite build --mode development",
    "build": "vite build",
    "dev": "vite",
    "format": "prettier -w .",
    "lint": "run-s lint:scripts lint:styles",
    "lint:scripts": "eslint .",
    "lint:styles": "stylelint **/*.{css,scss}",
    "lint:styles:fix": "stylelint **/*.{css,scss} --fix",
    "typecheck": "tsc -noEmit",
    "tsgo": "tsgo",
    "check-types": "tsgo --noEmit"
  },
